.table {  
    border: 0px solid darkgray;
 }
.thead {
    display: flex;
    width: 100%;
    justify-content: center;  
    height: 3rem;  
    align-items: center;
    font-size:11pt;
}
.tr {
    display: flex;
    width: 100%;
    justify-content: center;  
    height: 6rem;  
    align-items: center;
    font-size:9pt;
}
.th {  
    width: 17%;  
    justify-content: center;  
    background: #333;  
    color: #fff;  
    display: flex;  
    height: 3rem;  
    align-items: center;
}
.td {
    width:17%;    
    justify-content: center;    
    display: flex;  
    height: 6rem;
    /* text-align: center; */
    align-items: center;
}


.bg-w{  
    background: #ddd;
}
.bg-g{  
    background: rgb(184, 192, 196);
}



.rotateRight{
    transform: rotate(180deg);
}