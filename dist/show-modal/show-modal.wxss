
.modal {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 10000;
  text-align: center;
  background: rgba(0, 0, 0, 0.6);
  height: 100%;
  display: flex;
  align-items: center;
}

.panel {
  position: absolute;
  align-self: center;
  left: 7.5%;
  width: 85%;
  min-height: 300rpx;
  box-sizing: border-box;
  background-color: #f8f8f8;
}
